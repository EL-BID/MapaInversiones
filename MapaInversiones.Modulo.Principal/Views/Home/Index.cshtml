    <!-- NEWS AND SOCIAL MEDIA-->
    <section class="section">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="Cnews">
                        <div class="section-heading heading-left">
                            <a href="https://www.cali.gov.co/publicaciones/noticias/?tema=2" target="_blank">
                                <h2>¡Actualizáte!</h2>
                            </a>
                            <div class="section-subheading">
                                Conocé las últimas noticias sobre la ejecución de los proyectos en Cali.
                            </div>
                        </div>
                        <div class="cards-News-container">
                            @foreach (var noticia in await Orchard.GetRecentContentItemsByContentTypeAsync("BlogPost", 4))
                            {

                                <!--NOTICIA 3-->
                                <div class="card mb-3">
                                    <div class="row g-0">
                                        <div class="col-md-4 col-lg-3">
                                            @{
                                                var imgpath = "img/Pic001.jpg";
                                                if (noticia.Content.BlogPost.BannerImage.Paths.Count != 0)
                                                {
                                                    imgpath = noticia.Content.BlogPost.BannerImage.Paths[0];
                                                    <img class="img-fluid" src=@Orchard.AssetUrl((string)imgpath) alt="...">
                                                }
                                                else
                                                {
                                                    <img class="img-fluid" src=@imgpath alt="...">
                                                }
                                            }

                                        </div>
                                        <div class="col-md-8 col-lg-9">
                                            <div class="card-body Noticia">
                                                <a href="/Noticia/@noticia.ContentItemId"><span class="h4">@noticia</span></a>
                                                <p class="card-text">@noticia.Content.BlogPost.Subtitle.Text</p>
                                                <!--<p class="card-text"><small class="text-muted">Creado: @noticia.CreatedUtc</small></p>-->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                    <div class="section-btns justify-content-center py-3">
                        <a title="Leer más noticias" href="https://www.cali.gov.co/publicaciones/noticias/?tema=2" class="btn btn-primary" target="_blank">
                            <span>Leer más noticias</span>
                            <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                                <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--CONTACT US-->
    <!--HELP CENTER SHORTCUT-->
    <div id="helpCenter"></div>


</main>
@section scripts
{
    <script type="text/javascript" src="~/js/lib/d3/d3.v7.min.js"></script>
    <script type="text/javascript" src="~/js/search_aux.js"></script>
    <script type="text/javascript" src="~/js/lib/d3/d3plus@2.js"></script>
    <script src="~/js/scripts.js"></script>
    <script src="~/js/home_aux.js"></script>
}
