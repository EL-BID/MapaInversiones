@inject OrchardCore.IOrchardHelper Orchard
@model PlataformaTransparencia.Modelos.ModelHomeData

@{
    System.Globalization.CultureInfo cultureRD = new System.Globalization.CultureInfo("es-CO");
}

@{

    var banners = await Orchard.GetContentItemByHandleAsync("alias:bannerhome");
    var content_media = await Orchard.GetContentItemByHandleAsync("alias:VideoHome");
    var url_aux = Convert.ToString(content_media?.Content?.BannerMedia?.src?.Url);

    var maxAnyo = @Model.MaxAnnioEntidades;
    var anteriorVigencia = @Model.anteriorVigencia;
    var flag_vigencia_anterior = Model?.InfoVigenciaAnterior != null;
    var vigente_anterior = @String.Format("{0:n2}", Model?.InfoVigenciaAnterior?.Presupuesto != null ? Model.InfoVigenciaAnterior.Presupuesto / 1000000000000 : 0);
    var porc_anterior = Model.InfoVigenciaAnterior.Presupuesto > 0 ? @String.Format("{0:n0}", (Model.InfoVigenciaAnterior.Ejecutado / Model.InfoVigenciaAnterior.Presupuesto) * 100) : "0";
    var vigente_actual = @String.Format("{0:n2}", Model?.InfoVigenciaActual?.Presupuesto != null ? Model.InfoVigenciaActual.Presupuesto / 1000000000000 : 0);
    var porc_actual = Model.InfoVigenciaActual.Presupuesto > 0 ? String.Format("{0:n0}", (Model.InfoVigenciaActual.Ejecutado / Model.InfoVigenciaActual.Presupuesto) * 100): "0";
    var AprobadoFondo = @String.Format("{0:n2}", Model?.InfoVigenciaAnterior?.AprobadoFondoPropio != null ? Model.InfoVigenciaAnterior.AprobadoFondoPropio / 1000000000000 : 0);
}
<style>
    .capitalEntidad {
    text-transform: capitalize;
    }

    .autocomplete-suggestions {
    border: 1px solid #999;
    background: #FFF;
    overflow: auto;
    }

    .autocomplete-suggestion {
    padding: 2px 5px;
    white-space: nowrap;
    overflow: hidden;
    }

    .autocomplete-selected {
    background: #F0F0F0;
    }

    .autocomplete-suggestions strong {
    font-weight: normal;
    color: #3399FF;
    }

    .autocomplete-group {
    padding: 2px 5px;
    }

    .autocomplete-group strong {
    display: block;
    border-bottom: 1px solid #000;
    }

    path:hover {
    fill: #5916c3;
    stroke: #5916c3;
    stroke-width: 3px;
    transition: fill 0.4s;
    color:#fff;
    }

    /* Estilos específicos para la gráfica */
    .bubble-chart-container {
    width: 100%;
    height: auto;
    position: relative;
    }

    .bubble-chart-svg {
    width: 100%;
    height: auto;
    }

    .bubble-chart-bubble {
    stroke: #fff;
    stroke-width: 1px;
    stroke-linejoin: round;
    transition: transform 0.2s ease, stroke-width 0.2s ease, stroke 0.2s ease;
    }

    .bubble-chart-text {
    font-family: inherit;
    fill: #fff;
    font-weight: bold;
    pointer-events: none;
    text-anchor: middle;
    }

    .bubble-chart-tooltip {
    position: absolute;
    text-align: center;
    padding: 10px;
    font-family: inherit;
    font-size: clamp(10px, 1vw, 12px);
    background: #dcdcdc;
    color: #000;
    border-radius: 10px;
    pointer-events: none;
    visibility: hidden;
    box-shadow: 0px 6px 20px rgba(0, 0, 0, 0.25);
    z-index: 10;
    min-width: 100px;
    max-width: 200px;
    word-wrap: break-word;
    }
</style>

<main>
    <section class="section welcome">
        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-lg-6">
                    <h1>¿Qué es ¡Pa’ qué Veás!?</h1>
                    <p class="">Bienvenido (a) a ¡Pa’ que Veás!, el monitor de Inversión Pública de la Alcaldía de Cali, donde podrás conocer el avance de los proyectos que se desarrollan en cada comuna y corregimiento de la ciudad con dinero público, filtrándolos por zona, sector de la inversión, dependencia gestora y estado del proyecto. </p>
                    <p class="text-bold">¡Gracias por tu compromiso con el control social para recuperar a Cali!</p>
                    <div class="wrap-cta my-3">
                        <a href="/Acercade" target="_blank" class="btn btn-outlined btn-lg">
                            <span title="Ver más">Conocé más </span>

                            <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                                <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                            </svg>
                        </a>
                    </div>
                </div>
                @if (!string.IsNullOrWhiteSpace(url_aux))
                {
                    <div class="col-lg-4">
                        <div class="ratio ratio-16x9">
                            <iframe width="560" height="315" src="@url_aux" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                        </div>
                    </div>
                }


            </div>
        </div>
    </section>
    <!-- MAIN SLIDER SECTION -->
    <input id="anioPresupuesto" value="@maxAnyo" hidden />
    <input id="anteriorVigencia" value="@anteriorVigencia" hidden />
    <section class="section main-slider" id="ContenedorVigencia">
        @if (flag_vigencia_anterior)
        {
            <div class="container">
                <div class="row wrap-header-tit justify-content-center">

                    <div class="col-lg-10 Tagline">
                        <h2>¿En qué invirtió la Alcaldía de Cali en @anteriorVigencia?</h2>

                    </div>
                    <div class="col-lg-10 text-center">
                        <span>En ¡Pa' que Veás! podrás conocer los proyectos que se adelantaron con plata pública en la ‘Sucursal del Cielo’ el año pasado.</span><br />
                        <span>¡Quedáte y revisá cómo se hicieron las cosas en tu comuna o corregimiento!</span>
                    </div>

                </div>


            </div>
            <div class="container-fluid">
                <!-- MAIN SLIDESHOW-->
                <div class="slide-vertical-thumb">
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-xl-3 col-lg-3 col-xxl-3 thumb-left">
                            <div class="carousel carousel-nav " data-flickity='{"asNavFor": ".carousel-mainX", "draggable": false, "percentPosition": false, "groupCells": "100%", "pageDots": false, "prevNextButtons": false }'>
                                <div class="carousel-cell"> <div>¿Cuánta fue la plata disponible?</div></div>
                                <div class="carousel-cell"> <div>¿En qué se invirtió la plata de los caleños?</div></div>
                                <div class="carousel-cell"><div> ¿Quién invirtió?</div></div>

                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-xl-9 col-lg-9 col-xxl-9 slide-horizontal">
                            <div class="carousel carousel-mainX" data-flickity='{ "contain": true, "pageDots": false, "autoPlay": 9000, "adaptiveHeight": "true", "groupCells": true }'>
                                <div class="carousel-cell">
                                    <div class="wrap-cell-container">
                                        <div class="row align-items-center">
                                            <div class="col-lg-12"><h3 class="py-3">¿Cuánta fue la plata disponible?</h3></div>
                                            <div class="col-md-6 col-lg-5 h-100">
                                                <img class="img-s1 img-fluid" src="img/img-slide1.png" alt="Grupo de personas reunidas en un taller de participación ciudadana  de la Alcaldía de Cali" />
                                            </div>
                                            <div class="col-sm-12 col-md-6 col-lg-7 h-100">
                                                <div class="wrap-main-datos">
                                                    <div class="wrap-summary">
                                                        <span class="big-number">$ @vigente_anterior billones</span>
                                                        <small>Presupuesto Total (incluyendo recursos que aporta el Gobierno nacional – SGP-)</small>
                                                    </div>
                                                    <div class="wrap-summary">
                                                        <span class="big-number">$ @AprobadoFondo billones</span>
                                                        <small>Recursos Propios</small>
                                                    </div>

                                                    <div class="wrap-summary">
                                                        <span class="big-number">@porc_anterior %</span>
                                                        <small>Ejecutado</small>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                    </div>

                                </div>
                                <div class="carousel-cell">
                                    <div class="wrap-cell-container">
                                        <div class="row align-items-center">
                                            <div class="col-lg-12">
                                                <h3>¿En qué se invirtió la plata de los caleños?</h3>

                                                <p>Descubrí los sectores en que se invirtieron los recursos propios de la Alcaldía, sin SGP (recursos que llegan desde el Gobierno nacional) </p>

                                            </div>
                                            <div class="col-md-6 col-lg-6 col-xl-6 col-xxl-5 h-100" style="text-align:left;"> <div id="divEnQueInvirtieron" style="z-index:1000000;width:100%;height:auto;"></div></div>
                                            <div class="col-md-6 col-lg-6 col-xl-6 col-xxl-7  h-100">

                                                <div class="wrap">
                                                    <p class="text-bold"><span class="text-bold">SECTORES</span> con mayor asignación presupuestal para inversión durante @anteriorVigencia:</p>
                                                    <div class="row" id="cardEnQueInvirtieron"> </div>
                                                </div>
                                                <div class="wrap-cta my-3">
                                                    <a href="/BusquedaResultados?Type=Sector" class="btn btn-primary btn-lg">
                                                        <span title="Ver más">Ver todos los sectores </span>

                                                        <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                                                            <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                                                        </svg>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>


                                    </div>

                                </div>
                                <div class="carousel-cell">
                                    <div class="wrap-cell-container">
                                        <div class="row align-items-center">
                                            <div class="col-lg-12">
                                                <h3>¿Quién invirtió?</h3>
                                                <p>Explorá la gestión y ejecución presupuestal de los entes gestores (secretarías, departamentos y unidades de la Alcaldía).</p>
                                            </div>
                                            <div class="col-md-6 col-lg-6  col-xl-6 col-xxl-5 h-100"> <div id="divQuienInvirtio" class="bubble-chart-container" style="z-index: 1000000;"></div></div>

                                            <div class="col-md-6 col-lg-6 col-xl-6 col-xxl-7 h-100">

                                                <div class="wrap">
                                                    <p class="text-bold"><span class="text-bold">ENTES GESTORES</span> con mayor ejecución presupuestal durante el @anteriorVigencia</p>
                                                    <div class="row" id="cardQuienInvirtio">
                                                    </div>
                                                </div>
                                                <div class="wrap-cta my-3">
                                                    <a href="/BusquedaResultados?Type=Ente" class="btn btn-primary btn-lg">
                                                        <span title="Ver más">Ver Entes Gestores </span>

                                                        <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                                                            <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                                                        </svg>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>


                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        
        
        }
        
    </section>


    <!-- COMO VA LA EJECUCIóN 2025 -->
    
    
    <section class="section wrap-instituciones soft-gradient">
        <div class="container">
            <div class="row align-items-center justify-content-center mb-4" id="divVigenciaActual">
                <div class="section-heading heading-center py-4">
                    <h2>¿Cómo va la ejecución del presupuesto en <span class="anio">@maxAnyo?</span></h2>
                </div>
                <div class="col-lg-12">
                    <div class="wrap-icon text-center">
                        <img class="img-fluid" src="img/icon-budget.svg" alt="icono decorativo" />
                    </div>
                    <div class="wrap-summary mb-4">
                        <span class="h5">Recursos disponibles</span>
                        <span class="big-number">$ @vigente_actual billones</span>
                        
                    </div>

                </div>
                <div class="col-lg-8">
                    <div class="wrap-summary mb-4">
                        <span class="h5">Ejecutado con fecha de corte: @Model.fechasCorte.fechaCorte</span>
                        <div class="progesscontainer pgreen">
                            <div class="progress" style="height:65px;">
                                <div class="bg-proexec" role="progressbar"
                                     style='@($"width:{porc_actual}%")'
                                     aria-valuenow='@(porc_actual)'
                                     aria-valuemin="0"
                                     aria-valuemax="100">
                                    @(porc_actual)%
                                </div>
                            </div>
                        </div>

                    </div>
                    
                </div>
                <div class="sep-H-shadow"></div>
            </div>
            <!-- INSTITUCIONES -->
            <div class="row py-4">
                <div class="col-12">
                    <div class="section-heading heading-center py-4">
                        <h2>¿Cuánto presupuesto ejecutan secretarías, departamentos y unidades de la Alcaldía durante el <span class="anio">@maxAnyo?</span></h2>
                        <div class="section-subheading">
                        </div>
                    </div>
                </div>
                <div class="col-12">
                    <div id="divContenedorFichas22" class="project-carusel" data-flickity='{ "imagesLoaded": true, "pageDots": false, "lazyLoad": true, "lazyLoad": 10, "groupCells": true, "contain": true, "prevNextButtons": true, "autoPlay": 8000 }'>
                        @if (Model.Entidades != null)
                        {
                            @foreach (var item in Model.Entidades)
                            {
                                var porcentaje = "0%";
                                var anyo = @Model.MaxAnnioEntidades;
                                var href_aux = "/perfilEntidad?codEntidad=" + item.id;
                                porcentaje = @item.porcentaje.ToString().Replace(',', '.') + "%";

                                <text>
                                    <div class="col-lg-3 mb-3">
                                        <div class="card h-100 shadow border-0 card-entidad">
                                            <a href=@href_aux>
                                                <div class="card-body">
                                                    <div class="card-title-container mh56">
                                                        <span class="h5" id="@item.id">@item.labelGroup</span>
                                                    </div>
                                                    <div class="wrap-desc-entidad">
                                                        <div class="execute">Valor total ejecutado</div>
                                                        <div class="progress">
                                                            <div class="progress-bar" aria-label="Porcentaje Ejecutado" role="progressbar"
                                                                 style="width:@porcentaje" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                                                        </div>
                                                        <div class="wrap-data-advance py-3">
                                                            <span class="executeN">$ @String.Format(cultureRD, "{0:n0}", item.avance / 1000000)</span><span class="executeV"> millones</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </text>
                            }
                        }
                    </div>
                </div>

            </div>
            <div class="section-btns justify-content-center py-3">
                <a title="Ver todos los entes gestores" href="/BusquedaResultados?Type=Ente" class="btn btn-primary">
                    <span>Ver todos los entes gestores</span>
                    <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                        <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                    </svg>
                </a>
            </div>
        </div>
    </section>
    <!-- MAPA COMUNAS Y CORREGIMIENTOS -->
    <section class="section provincias pb-0">
        <div class="container-fluid">
            <div class="row align-items-center justify-content-center">
                <div class="col-lg-8">
                    <div class="section-heading heading-center">
                        <h2>¿Qué proyectos hay en tu zona?</h2>
                        <p class="section-subheading">En ¡Pa' que Veás! te mostramos los proyectos que se adelantan en tu comuna o corregimiento.<br /> <span class="text-bold">¡En la jugada porque eso también es tuyo! Hacé clic y revisá</span></p>

                    </div>
                    <div class="wrap-counter d-flex">
                        <div class="counters">
                            <div class="desc-data">
                                <div class="executeV">Proyectos en ejecución</div>
                                <div class="executeN" id="numProyExe">@String.Format(cultureRD, "{0:n0}", Model.countOngoingProjects.cantidad)</div>
                            </div>
                        </div>
                        <div class="counters">
                            <div class="desc-data">
                                <div class="executeV">Costo total de proyectos en ejecución</div>
                                <div class="executeN" id="costoProyExce">$ @String.Format(cultureRD, "{0:n0}", Model.countOngoingProjects.costo / 1000000) millones</div>
                            </div>
                        </div>
                    </div>


                </div>


            </div>
            <div class="row location-tabs">
                <!--COMUNAS-->
                <div class="location-card mb-4">
                    <h3 class="text-center">Comunas</h3>
                    <div class="carousel" data-flickity='{ "contain": true, "pageDots": true, "autoPlay": 9000, "cellAlign": "right", "groupCells": true }'>
                        @foreach (var item in Model.Comunas)
                        {
                            <div class="carousel-cell">
                                <div class="card comuna" style="background-image:url(@Url.Content($"../img/{item.UrlImg}"))">
                                    <div class="card-body">
                                        <a href="@item.LinkLocationProfile" class="h4">@item.Nombre</a>
                                        <span class="number-proyecto">(@item.CantidadProyectos proyectos)</span>
                                    </div>
                                </div>
                            </div>
                        }
                                       
                    </div>
                </div>
                <!--CORRREGIMIENTOS-->
                <div class="location-card">
                    <h3 class="text-center">Corregimientos</h3>
                    <div class="carousel" data-flickity='{ "contain": true, "pageDots": true, "autoPlay": 9000, "cellAlign": "right", "groupCells": true }'>
                        @foreach (var item in Model.Corregimientos)
                        {
                            <div class="carousel-cell">
                                <div class="card comuna" style="background-image:url(@Url.Content($"../img/{item.UrlImg}")">
                                    <div class="card-body">
                                        <a href="@item.LinkLocationProfile" class="h4">@item.Nombre</a>
                                        <span class="number-proyecto">(@item.CantidadProyectos proyectos)</span>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <!--tabs-->
            <div class="row d-none">


                <div class="tabs location-tabs">
                    <ul class="tabs-nav">
                        <li class="active">Comunas</li>
                        <li class="">Corregimientos</li>

                    </ul>
                    <div class="tabs-container">
                        <div class="tabs-item location-card content active">
                        </div>
                        <!--tabs corregimiento-->
                        <div class="tabs-item location-card content">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section map-link">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8">
                        <div class="section-heading heading-center">
                            <h2>Explorá el Mapa de Proyectos</h2>
                            <div class="section-subheading">
                                Descubrí cómo se transforma tu comunidad a través de nuestro Mapa de Proyectos y conocé en detalle cómo se está utilizando la plata de todos para recuperar a Cali y mejorar tu entorno.
                            </div>
                        </div>
                        <div class="wrap-cta my-3">
                            <a href="/MapView" class="btn btn-primary btn-lg">
                                <span title="Ver más">Ver Mapa </span>

                                <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9">
                                    <use xlink:href="../../../img/sprite.svg#arrow-right"></use>
                                </svg>
                            </a>
                        </div>
                    </div>

                </div>



            </div>


        </div>
    </section>
    <!-- Call to action-->
